  // Maximum number of matches to decorate per regex.
  // Prevents the app from crashing with accidental cathastropic regexes
  "highlight.maxMatches": 250,
  "vetur.format.options.tabSize": 4,
  "vetur.completion.tagCasing": "initial",
  "vetur.format.defaultFormatter.ts": "none",
  "vetur.format.defaultFormatter.html": "none",
  "vetur.format.defaultFormatter.pug": "none",
  "javascript.preferences.quoteStyle": "double",
  "settingsSync.ignoredSettings": ["javascript.preferences.quoteStyle"],
  "[vue]": {
    "editor.defaultFormatter": "octref.vetur"
  },
  "vetur.format.defaultFormatter.js": "none",
  //   "editor.codeActionsOnSave": {
  //     "source.fixAll.eslint": "explicit"
  //   },
  "vetur.format.defaultFormatter.css": "none",
  "vetur.format.defaultFormatter.less": "none",
  "vetur.format.defaultFormatter.postcss": "none",
  "vetur.format.defaultFormatter.scss": "none",
  "json.format.enable": false,
  "workbench.productIconTheme": "fluent-icons",
  "workbench.editor.revealIfOpen": true,
  "highlight.regexes": {
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *TODO(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "filterFileRegex": ".*(?<!CHANGELOG.md)$",
      "decorations": [
        {
          "overviewRulerColor": "#9dc023",
          "backgroundColor": "transparent",
          "color": "#9dc023",
          "fontStyle": "italic",
          "fontWeight": "bold"
        },
        {
          "backgroundColor": "transparent",
          "color": "#9dc023",
          "fontStyle": "italic"
        }
      ]
    },
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:FIXME|FIX|BUG|UGLY|DEBUG|HACK)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "filterFileRegex": ".*(?<!CHANGELOG.md|COMMIT_EDITMSG)$",
      "decorations": [
        {
          "overviewRulerColor": "#fd6d7f",
          "backgroundColor": "transparent",
          "color": "#fd6d7f",
          "fontStyle": "italic",
          "fontWeight": "bold"
        },
        {
          "backgroundColor": "transparent",
          "color": "#fd6d7f",
          "fontStyle": "italic"
        }
      ]
    },
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:REVIEW|OPTIMIZE|TSC)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "filterFileRegex": ".*(?<!CHANGELOG.md)$",
      "decorations": [
        {
          "overviewRulerColor": "#E35FE3",
          "backgroundColor": "transparent",
          "color": "#E35FE3",
          "fontStyle": "italic",
          "fontWeight": "bold"
        },
        {
          "backgroundColor": "transparent",
          "color": "#E35FE3",
          "fontStyle": "italic"
        }
      ]
    },
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:IDEA)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "filterFileRegex": ".*(?<!CHANGELOG.md)$",
      "decorations": [
        {
          "overviewRulerColor": "#00ccff",
          "backgroundColor": "transparent",
          "color": "#00ccff",
          "fontStyle": "italic",
          "fontWeight": "bold"
        },
        {
          "backgroundColor": "transparent",
          "color": "#00ccff",
          "fontStyle": "italic"
        }
      ]
    },
    "((?:<!-- *)?(?:#|// @|//|./\\*+|<!--|--|\\* @|{!|{{!--|{{!) *(?:DEPRECATED)(?:\\s*\\([^)]+\\))?:?)((?!\\w)(?: *-->| *\\*/| *!}| *--}}| *}}|(?= *(?:[^:]//|/\\*+|<!--|@|--|{!|{{!--|{{!))|(?: +[^\\n@]*?)(?= *(?:[^:]//|/\\*+|<!--|@|--(?!>)|{!|{{!--|{{!))|(?: +[^@\\n]+)?))": {
      "filterFileRegex": ".*(?<!CHANGELOG.md)$",
      "decorations": [
        {
          "overviewRulerColor": "#cea234",
          "backgroundColor": "transparent",
          "color": "#cea234",
          "fontStyle": "italic",
          "fontWeight": "bold"
        },
        {
          "backgroundColor": "transparent",
          "color": "#cea234",
          "fontStyle": "italic"
        }
      ]
    }
  },
  "editor.tabSize": 2,
  "security.workspace.trust.untrustedFiles": "open",
  "[json]": {
    "editor.tabSize": 4,
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": ["document"]
  },
  "[jsonc]": {
    "editor.tabSize": 4,
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": ["document"]
  },
  "[yaml]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.autoIndent": "keep",
    "gitlens.codeLens.scopes": ["document"],
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    }
  },
  "[kotlin]": {
    "editor.insertSpaces": false,
    "editor.tabSize": 4
  },
  "[rhai]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[rust]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.autoClosingTags": false,
  "workbench.editor.wrapTabs": true,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "editor.accessibilitySupport": "off",
  "cSpell.ignoreWords": ["favorited", "wayfair"],
  "terminal.integrated.fontFamily": "'SF-Mono-Powerline-Regular', monospace",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "cSpell.userWords": ["Foid", "froid", "supergraph"],
  "rust-analyzer.check.command": "clippy",
  "debug.allowBreakpointsEverywhere": true,
  "diffEditor.maxComputationTime": 0,
  "diffEditor.ignoreTrimWhitespace": false,
  "console-ninja.featureSet": "Community",
  "editor.largeFileOptimizations": false,
  "redhat.telemetry.enabled": true,
  "docker.extension.enableComposeLanguageServer": false
